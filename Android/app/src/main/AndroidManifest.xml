<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aseman">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/main"
        android:label="@string/app_name"
        android:roundIcon="@drawable/main"
        android:supportsRtl="true"
        android:theme="@style/Theme.Aseman">
        <activity
            android:name=".SplashScreen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"/>
        <activity android:name=".Authentication.LoginActivity"/>
        <activity android:name=".Ticket.TicketActivity"/>
        <activity android:name=".Ticket.TicketDetailActivity"/>
        <activity android:name=".ManagerDaillyReports.ManagerDailyReportActivity"/>
        <activity android:name=".UnderManagmentUser.UnderManagementUserActivity"/>
        <activity android:name=".UnderManagmentUser.UserDetailActivity"/>
        <activity android:name=".UnderManagmentUser.UserScoringActivity"/>
    </application>

</manifest>